{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "Random number generator node for n8n using Random.org API",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "random",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelabras/desafio-estagio-onfly"
  },
  "main": "dist/nodes/Random.node.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "cpy nodes/*.svg dist/nodes",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Random.node.js"
    ]
  },
  "devDependencies": {
    "cpy-cli": "^6.0.0",
    "n8n-workflow": "^1.17.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.82.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rafaelabras/desafio-estagio-onfly/issues"
  }
}
